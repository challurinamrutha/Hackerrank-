#include <string.h>
#include<stdio.h>
#include<assert.h>
#include<math.h>
#include<stdbool.h>
#include<limits.h>
int main()
{
int a;
int b;
int c;
scanf("%d %d %d",&a,&b,&c);
int p;
int q;
int r;
int X = 0;
int Y = 0;
scanf("%d %d %d",&p,&q,&r);
if (a > p)
X++;
else if (a < p)
Y++;
else{}
if (b > q)
X++;
else if (b < q)
Y++;
else{}
if (c > r)
X++;
else if ( c < r)
Y++;
else{}
printf("%d %d",X,Y);
return 0;
}
